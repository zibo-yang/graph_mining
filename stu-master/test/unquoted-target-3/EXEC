#! /bin/sh
#
# Empty argument to -c
#

rm -f ? || exit 1

../../stu.test -c '' >list.out 2>list.err
exitcode="$?"

[ "$exitcode" = 4 ] || {
	echo >&2 '*** exit code'
	exit 1
}

diff list.err stderr-correct || {
	echo >&2 '*** stderr'
	echo >&2 list.err:
	echo >&2 ______
	cat >&2 list.err
	echo >&2 ______
	exit 1
}

exit 0
